{
  "name": "zxf/tools",
  "description": "第三方登录、条形码Barcode、二维码QrCode、生成菜单、发送短信、图片处理、随机数、字符压缩、文字转图片、生成数据字典、远程截图、Redis、laravel多模块应用和一些常用方法集合等",
  "keywords": [
    "database",
    "sms",
    "tools",
    "laravel",
    "redis",
    "tree",
    "office",
    "curl",
    "tn_code",
    "dom",
    "code"
  ],
  "homepage": "https://github.com/zhaoxianfang/",
  "license": "MIT",
  "support": {
    "source": "https://github.com/zhaoxianfang/tools"
  },
  "authors": [
    {
      "name": "zhaoxianfang",
      "email": "1748331509@qq.com",
      "homepage": "https://www.weisifang.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "zxf\\": "src"
    },
    "files": [
      "src/funcs/helpers.php",
      "src/funcs/module_helpers.php"
    ]
  },
  "config": {
    "process-timeout": 300
  },
  "extra": {
    "laravel": {
      "providers": [
        "zxf\\Laravel\\LaravelModulesServiceProvider"
      ],
      "aliases": {
        "Module": "zxf\\Facades\\Module"
      }
    }
  }
}
