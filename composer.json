{
  "name": "zxf/tools",
  "description": "第三方登录、条形码Barcode、二维码QrCode、生成菜单、发送短信、图片处理、随机数、字符压缩、文字转图片、生成数据字典、远程截图、Redis、laravel多模块应用和一些常用方法集合等",
  "keywords": [
    "mysqli",
    "php",
    "sms",
    "tools",
    "laravel",
    "redis",
    "tree",
    "js压缩",
    "css压缩",
    "命令行参数解析",
    "类库",
    "工具"
  ],
  "homepage": "https://github.com/zhaoxianfang/",
  "license": "Apache-2.0",
  "support": {
    "source": "https://github.com/zhaoxianfang/tools"
  },
  "authors": [
    {
      "name": "zhaoxianfang",
      "email": "1748331509@qq.com",
      "homepage": "https://weisifang.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-iconv": "*"
  },
  "autoload": {
    "psr-4": {
      "zxf\\": "./src"
    },
    "files": [
      "src/funcs/helpers.php",
      "src/funcs/module_helpers.php"
    ]
  },
  "config": {
    "process-timeout": 300
  },
  "extra": {
    "laravel": {
      "providers": [
        "zxf\\laravel\\ServiceProvider"
      ],
      "aliases": {
        "Module": "zxf\\Facades\\Module"
      }
    }
  }
}
